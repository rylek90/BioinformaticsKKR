<mui:ModernWindow 
    x:Class="BioinformaticsKKR.View.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        Title="MainWindow" Height="450" Width="800"
    ContentSource="/View/ReadFileView.xaml">
   <mui:ModernWindow.MenuLinkGroups>
       <mui:LinkGroup DisplayName="Main">
           <mui:LinkGroup.Links>
               <mui:Link DisplayName="Read File" Source="/View/ReadFileView.xaml" />
               <mui:Link DisplayName="Converter" Source="/View/ConvertView.xaml"/>
               <mui:Link DisplayName="Align" Source="/View/AlignView.xaml"/>
               <mui:Link DisplayName="Assemble" Source="/View/AssembleView.xaml"/>
           </mui:LinkGroup.Links>
       </mui:LinkGroup>
   </mui:ModernWindow.MenuLinkGroups>
    
</mui:ModernWindow>
